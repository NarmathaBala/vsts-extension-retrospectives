{
  "name": "azdo",
  "version": "1.0.0",
  "description": "azdo",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "set NODE_ENV=production && rm -rf ./dist && webpack --mode=production",
    "lint": "eslint . --ext .ts,.tsx",
    "pack:d": "tfx extension create --manifests vss-extension-dev.json --rev-version --output-path ./dist/",
    "pack:p": "tfx extension create --manifests vss-extension-prod.json --rev-version --output-path ./dist/",
    "publish:d": "npm run build && npm run pack:d && tfx extension publish --manifests vss-extension-dev.json --vsix ./dist/*.vsix",
    "publish:p": "npm run build && npm run pack:p && tfx extension publish --manifests vss-extension-prod.json --vsix ./dist/*.vsix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "~2.164.0",
    "moment": "2.29.1",
    "office-ui-fabric-react": "7.173.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "0.28.1",
    "typescript": "^4.5.4",
    "react-toastify": "7.0.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "5.0.1",
    "eslint": "7.30.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.24.0",
    "file-loader": "~2.0.0",
    "moment-locales-webpack-plugin": "1.2.0",
    "rimraf": "~2.6.2",
    "sass": "^1.46.0",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "tfx-cli": "^0.6.3",
    "ts-loader": "8.0.14",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "4.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx|jsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|azure-devops-ui|azure-devops-extension-sdk)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "preset": "ts-jest/presets/js-with-babel",
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "cobertura"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    }
  }
}
